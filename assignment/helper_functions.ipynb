    "def load_samples(run_name, filename='samples.txt'): # Adapted to the possibility to have a organized directory structure...\n",
    "    path = BASE_DIR / \"out\" / run_name / filename\n",
    "    if not path.exists(): return []\n",
    "    raw = path.read_text()\n",
    "    return [s.strip() for s in raw.split('\\n\\n') if s.strip()]\n",
    "\n",
    "def summarize_diversity(run_names):\n",
    "    print(f\"{'run':<15}{'#samples':<10}{'distinct-1':<12}{'distinct-2':<12}{'TTR':<8}\\n\")\n",
    "    for name in run_names:\n",
    "        samples = load_samples(name)\n",
    "        if not samples: continue\n",
    "        d1 = distinct_n(samples,1); d2 = distinct_n(samples,2); ttr = type_token_ratio(samples)\n",
