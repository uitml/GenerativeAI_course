    "def parse_log_file(log_path):\n",
    "    \"\"\"Parse nanoGPT log.txt and return dict with arrays for iter, train_loss, val_loss.\"\"\"\n",
    "    iters = []\n",
    "    train_losses = []\n",
    "    val_losses = []\n",
    "\n",
    "    if not log_path.exists():\n",
    "        print(f\"[WARN] No log found at {log_path}\")\n",
    "        return None\n",
    "\n",
    "    with log_path.open(\"r\", encoding=\"utf-8\") as f:\n",
    "        for line in f:\n",
    "            # Very simple, students can adapt for their exact format\n",
    "            # Example line patterns they might log:\n",
    "            # \"iter 1000: train loss 1.23, val loss 1.45\"\n",
    "            # \" iter 80: loss 2.3300, time 30.32ms, mfu 15.17%\n",
    "            m = re.search(r\"iter\\s+(\\d+):\\s+loss\\s+([0-9]*\\.?[0-9]+)\", line)\n",
    "            if m:\n",
    "                it = int(m.group(1))\n",
    "                tr = float(m.group(2))\n",
    "                # va = float(m.group(3))\n",
    "                iters.append(it)\n",
    "                train_losses.append(tr)\n",
    "                # val_losses.append(va)\n",
    "\n",
    "    if not iters:\n",
    "        print(f\"[WARN] No lines matched in {log_path}\")\n",
    "        return None\n",
    "\n",
    "    return {\n",
    "        \"iter\": np.array(iters),\n",
    "        \"train_loss\": np.array(train_losses),\n",
    "        # \"val_loss\": np.array(val_losses),\n",
    "    }\n",
    "\n",
    "\n",
    "parse_log_file(Path(BASE_DIR,'out-tinystories-train/log.txt'))\n",
    "\n"
   ],
